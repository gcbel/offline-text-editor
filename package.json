{
  "name": "Text Editor",
  "version": "1.0.0",
  "description": "Text editor that can be accessed both online and offline",
  "main": "server.js",
  "scripts": {
    "start:dev": "concurrently \"npm run server\" \"cd client && npm run dev\"",
    "start": "node server.js",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "install": "cd client && npm install",
    "client": "cd client && npm start"
  },
  "keywords": [],
  "author": "gcbel",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "idb": "^6.1.2"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
